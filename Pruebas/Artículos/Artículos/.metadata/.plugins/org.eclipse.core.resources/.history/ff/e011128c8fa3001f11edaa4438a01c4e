package com.miempresa.articulos.repository;

import org.springframework.data.repository.CrudRepository;
import org.springframework.data.repository.PagingAndSortingRepository;

import com.miempresa.articulos.entity.Articulo;

public interface IArticuloRepository extends PagingAndSortingRepository<Articulo, Long>, CrudRepository<Articulo, Long>{
	Iterable<Articulo> findAllByCategoria(String categoria);
}
