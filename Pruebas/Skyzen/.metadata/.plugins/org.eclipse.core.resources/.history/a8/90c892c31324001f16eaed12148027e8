package skyzen;

public enum AfricanCountriesActivities {
    PK("Kruger", "Safari en vehículo para avistar a los \"Big Five\" y caminata guiada para explorar la flora y fauna locales", 40, "/AfricanImages/AI1.jpg"),
    BRC("Blyde River Canyon", "Visita al Mirador de God’s Window para disfrutar de vistas panorámicas y paseo en barco por la represa del cañón", 70, "/AfricanImages/AI2.jpg"),
    PNP("Pilanesberg", "Safari en vehículo para avistar la vida silvestre y visita al centro de interpretación para aprender sobre la ecología del parque", 120, "/AfricanImages/AI3.jpg"),
    SS("Stellenbosch", "Tour de una bodega con cata de vinos y paseo por el centro histórico para explorar la arquitectura colonial", 160, "/AfricanImages/AI4.jpg"),
    HS("Hermanus", "Paseo en barco para avistar ballenas y caminata por el acantilado para disfrutar de las vistas panorámicas", 220, "/AfricanImages/AI5.jpg"),
    CS("Cederberg", "Caminata para ver las formaciones rocosas y visita a los sitios de arte rupestre para explorar la historia local", 120, "/AfricanImages/AI6.jpg"),
    PND("Drakensberg", "Caminata corta en Tugela Gorge para disfrutar de vistas y visita a las pinturas rupestres san para aprender sobre la cultura local", 220, "/AfricanImages/AI7.jpg"),
    RM("Mkhaya", "Safari en vehículo para avistar rinocerontes y visita a la reserva de elefantes para aprender sobre la conservación", 170, "/AfricanImages/AI8.jpg"),
    VRF("Valle del Río Fish", "Visita al mirador del Cañón del Río Fish para disfrutar de vistas panorámicas y caminata corta por el borde del cañón", 150, "/AfricanImages/AI9.jpg"),
    SN("Sossusvlei", "Subida a la Duna 45 para obtener vistas panorámicas y paseo por Deadvlei para explorar las dunas y árboles secos", 50, "/AfricanImages/AI10.jpg"),
    IB("Isla de Bazaruto", "Atisva las profundidades del océano iniciándote en el snorkel en los arrecifes de coral y paseo por las dunas de arena para disfrutar de la belleza natural", 220, "/AfricanImages/AI11.jpg"),
    PNS("Serengeti", "Súbete a un safari en vehículo para observar la vida silvestre y visita a un kopje para explorar las formaciones rocosas", 250, "/AfricanImages/AI12.jpg"),
    ACN("Ngorongoro", "Contrata uno de los múltiples safaris en el cráter para avistar animales y visita al mirador del cráter para disfrutar de vistas panorámicas", 60,"/AfricanImages/AI13.jpg"),
    PNK("Kilimanjaro", "Sumérgete en la cultura local realizando una caminata corta para explorar la base de la montaña y visita a una cascada para disfrutar de la naturaleza", 270, "/AfricanImages/AI14.jpg"),
    ZB("Zanzíbar", "Exploración de Stone Town para conocer la historia y cultura locales y tiempo libre en la playa para relajarse", 320, "/AfricanImages/AI15.jpg"),
    LM("Lamu", "Caminata por el casco antiguo para explorar la arquitectura y cultura swahili y tiempo libre en la playa para nadar y relajarse", 250, "/AfricanImages/AI16.jpg"),
    RG("Gedi", "Visita a las ruinas para explorar la historia y cultura swahili y caminata por el bosque para observar la vida silvestre", 200, "/AfricanImages/AI17.jpg"),
    PNV("P.N. Volcanes", "Realiza un trekking para observar gorilas de montaña y visita a un pueblo para conocer la cultura local", 250, "/AfricanImages/AI18.jpg"),
    PNB("Bwindi", "Realiza una ruta para observar gorilas de montaña y visita a una comunidad local para aprender sobre su estilo de vida", 220, "/AfricanImages/AI19.jpg"),
	IS("Isla de Socotra", "Explora su flora y fauna únicas en el mundo, incluidos los famosos árboles de sangre de dragón y las formas de vida marina", 250, "/AfricanImages/AI20.jpg"),
	M("Madagascar", "Las extraordinarias fauna y flora de la isla se combinan con paisajes épicos de una diversidad increíble: cañones calcáreos, karsts graníticos, colinas fértiles sembradas de arrozales en terraza, bosques de todo tipo y un suelo rico en laterita que ha dado al país su sobrenombre de “Isla Roja”.", 300, "/AfricanImages/AI21.jpg");
	


    private String lugar;
    private String descripcion;
    private int duracion;
    private String imagenes;

    private AfricanCountriesActivities(String lugar, String descripcion, int duracion, String imagenes) {
		this.lugar = lugar;
		this.descripcion = descripcion;
		this.duracion = duracion;
		this.imagenes = imagenes;
	}

	public String getLugar() {
		return lugar;
	}

	public String getDescripcion() {
		return descripcion;
	}

	public int getDuracion() {
		return duracion;
	}

	public String getImagenes() {
		return imagenes;
	}
	
}

